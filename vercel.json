{
  "buildCommand": ":",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/upsc-planner-final.html"
    },
    {
      "source": "/manage",
      "destination": "/upsc-planner-manage.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/config.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
